{
  "name": "@social-sdk/configs",
  "version": "0.0.0",
  "private": true,
  "description": "Shared ESLint, Prettier, and TypeScript configurations for the Social SDK",
  "homepage": "https://social-sdk.js.org",
  "bugs": {
    "url": "https://github.com/socialmated/social-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/socialmated/social-sdk.git",
    "directory": "packages/configs"
  },
  "license": "MIT",
  "author": "SocialMated",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.lib.json",
    "dev": "rm -rf dist && tsc -p tsconfig.lib.json --watch",
    "format": "prettier --write",
    "lint": "eslint --fix",
    "test": "vitest run",
    "test:ci": "CI=1 vitest run --coverage",
    "test:coverage": "vitest run --coverage",
    "test:snapshot": "vitest run --update",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "1.10.4",
    "@babel/core": "7.27.1",
    "@babel/eslint-parser": "7.27.1",
